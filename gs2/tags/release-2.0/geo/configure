#! /bin/sh





# remove /usr/afsws/bin from the path, since the stooooopid Cray
# setup has a useless /usr/afsws/bin/ranlib that messes things up
fc_save_PATH=$PATH
fc_save_IFS=$IFS
fc_new_PATH=
IFS=:
for fc_dir in $PATH
do
  test -z "$fc_dir" && fc_dir=.
  case "$fc_dir" in
    /usr/afsws/bin*)
      ;;
    *)
      test -z "$fc_new_PATH" || fc_new_PATH="$fc_new_PATH:"
      fc_new_PATH="$fc_new_PATH$fc_dir"
      ;;
  esac
done
IFS=$fc_save_IFS
PATH=$fc_new_PATH

